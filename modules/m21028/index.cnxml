          <colspec colnum="2" colname="c2"/>
          <colspec colnum="3" colname="c3"/>
          <colspec colnum="4" colname="c4"/>
          <colspec colnum="5" colname="c5"/>
          <colspec colnum="6" colname="c6"/>
          <tbody>
            <row>
              <entry>Name</entry>
              <entry>
                <emphasis effect="bold">Jump 1 </emphasis>
                <emphasis effect="italics">m</emphasis>
              </entry>
              <entry>
                <emphasis effect="bold">Jump 2</emphasis>
                <emphasis effect="italics">m</emphasis>
              </entry>
              <entry>
                <emphasis effect="bold">Jump 3</emphasis>
                <emphasis effect="italics">m</emphasis>
              </entry>
              <entry>
                <emphasis effect="bold">Jump 4</emphasis>
                <emphasis effect="italics">m</emphasis>
              </entry>
              <entry>
                <emphasis effect="bold">Best Jump</emphasis>

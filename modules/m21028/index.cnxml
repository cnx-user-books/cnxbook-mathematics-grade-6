              <entry>3,48</entry>
              <entry>3,02</entry>
              <entry>2,9</entry>
              <entry>____________</entry>
            </row>
            <row>
              <entry>Lala</entry>
              <entry>3,2</entry>
              <entry>3,04</entry>
              <entry>2,86</entry>
              <entry>3,4</entry>

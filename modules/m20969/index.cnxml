        <para id="id17099110">1.3</para>
        <figure id="id4146518">
          <media id="id4146518_media" alt="">
            <image mime-type="image/png" src="../../media/graphics3-9d6b.png" id="id4146518__onlineimage" height="183" width="297"/>
          </media>
        </figure>
